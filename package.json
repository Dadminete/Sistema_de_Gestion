{
  "name": "mi-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently --names \"FRONTEND,BACKEND\" --prefix \"[{name}]\" --prefix-colors \"cyan,green\" --kill-others-on-fail \"npm:start:frontend\" \"npm:start:backend\"",
    "start:frontend": "vite --host 0.0.0.0",
    "start:backend": "npm run dev --prefix server",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
